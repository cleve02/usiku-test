    "@types/express": "^4.17.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^2.7.1",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@colyseus/arena": "^0.14.13",
    "@colyseus/monitor": "^0.14.0",
    "@colyseus/social": "^0.10.9",
    "colyseus": "^0.14.0",
    "cors": "^2.8.5",
    "ejs": "^3.1.6",
    "express": "^4.16.4",
    "serve-index": "^1.8.0",
    "superagent": "^3.8.1",
